// VSCode workspace settings that are shared among all users of this project.
// This only affects subdirectories of this project.
{
  "html.format.enable": false,
  "githubPullRequests.ignoredPullRequestBranches": ["master"],
  "files.trimTrailingWhitespace": true,
  "dart.flutterSdkPath": "~/development/flutter/bin",
  "dart.addSdkToTerminalPath": true,
  "dart.sdkPath": "~/development/flutter/bin/cache/dart-sdk",
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "never",
    "source.fixAll": "never"
  },
  "dart.runPubGetOnPubspecChanges": "never",
  "dart.showTodos": false
}
